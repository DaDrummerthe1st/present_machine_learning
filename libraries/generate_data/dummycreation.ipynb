   "source": [
    "import random\n",
    "\n",
    "with open(\"matrix.txt\", \"w\") as f:\n",
    "    # generate 10 000 rows\n",
    "    item = random.choice([0,1])\n",
    "    for i in range(10000):\n",
    "        row = \"\"\n",
    "        for j in range(10):\n",
    "            row += str(item) + \", \"\n",
    "        f.write(row + \"\\n\")"
   ],
